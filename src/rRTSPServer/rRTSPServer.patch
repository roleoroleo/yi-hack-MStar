+size_t ByteStreamFileSource::cb_read(int stream, void *ptr, size_t size,
+    const void *buf, size_t buf_size)
+{
+    size_t ret;
+    off_t pos;
+
+    if (size <= 0) return 0;
+    pos = lseek(stream, 0, SEEK_CUR);
+    if (buf_size - pos >= size) {
+        ret = read(stream, ptr, size);
+    } else {
+        ret = read(stream, ptr, buf_size - pos);
+        ret += read(stream, ((unsigned char *) ptr) + (buf_size - pos), size - (buf_size - pos));
+    }
+    return ret;
+}
+
+size_t ByteStreamFileSource::cb_fread(void *ptr, size_t size,
+    const void *buf, size_t buf_size, FILE *stream)
+{
+    size_t ret;
+    long pos;
+
+    if (size <= 0) return 0;
+    pos = ftell(stream);
+    if (buf_size - pos >= size) {
+        ret = fread(ptr, sizeof(unsigned char), size, stream);
+    } else {
+        ret = fread(ptr, sizeof(unsigned char), buf_size - pos, stream);
+        ret += fread(((unsigned char *) ptr) + (buf_size - pos),
+            sizeof(unsigned char), size - (buf_size - pos), stream);
+    }
+    return ret;
+}
