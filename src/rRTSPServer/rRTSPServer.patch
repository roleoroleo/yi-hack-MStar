+                    if (debug) fprintf(stderr, "Found IDR HIGH\n");
+                    if (fResolution == RESOLUTION_HIGH) {
+                        process_frame = 1;
+                    } else {
+                        process_frame = 0;
+                    }
+                } else if (memcmp(buf_idx_1, PFR_START, sizeof(PFR_START)) == 0) {
+                    if (debug) fprintf(stderr, "Found PFR\n");
+//                    if (debug) fprintf(stderr, "%02x - %02x - %02x - %02x - %02x - %02x - %02x - %02x\n",
+//                        *buf_idx_1, *(buf_idx_1 + 1), *(buf_idx_1 + 2), *(buf_idx_1 + 3), *(buf_idx_1 + 4), *(buf_idx_1 + 5), *(buf_idx_1 + 6), *(buf_idx_1 + 7));
