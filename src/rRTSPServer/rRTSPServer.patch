+
+        switch (state) {
+            case STATE_SPS_HIGH:
+                if (memcmp(buf_idx_1, SPS_640X360, sizeof(SPS_640X360)) == 0) {
+                    state = STATE_SPS_LOW;
+                    if (debug) fprintf(stderr, "Found SPS LOW\n");
+                    fLastLow = 0;
+                    if (fResolution == RESOLUTION_LOW) {
+                        process_frame = 1;
+                    } else {
+                        process_frame = 0;
+                    }
+                } else if (memcmp(buf_idx_1, SPS_1920X1080, sizeof(SPS_1920X1080)) == 0) {
